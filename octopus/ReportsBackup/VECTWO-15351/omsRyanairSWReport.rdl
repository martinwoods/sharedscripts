  <Code>
Public Shared Function GetHeader(DateFrom As System.DateTime, DateTo As System.DateTime) As String
	Dim strRet As String
	Dim DateNull As System.DateTime = New System.DateTime(1000,1,1)

	If (DateFrom &gt; DateNull AndAlso DateTo &gt; DateNull) Then
		strRet = "Orders  between " + DateFrom.ToString("dd/MM/yyyy") + " and " + DateTo.ToString("dd/MM/yyyy") + "."
	Else If DateFrom &gt; DateNull Then
		strRet = "Orders after " + DateFrom.ToString("dd/MM/yyyy") + "."
	Else If DateTo &gt; DateNull Then
		strRet = "Orders before " + DateTo.ToString("dd/MM/yyyy") + "."
	Else
		strRet = "All orders."
	End If

	strRet = strRet + " Report generated on " + System.DateTime.Now.Date.ToString("dd/MM/yyyy")
	Return strRet
End Function


Public Shared Function StripHTML(htmlString As String) As String
	Dim pattern As String ="&lt;(.|" + vbCrLf + ")*?&gt;"
	Return System.Text.RegularExpressions.Regex.Replace(htmlString, pattern, String.Empty)
End Function

